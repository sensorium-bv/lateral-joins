{
    "name": "sensorium/lateral-joins",
    "description": "Extends Laravel query builder with lateral joins",
    "homepage": "https://www.sensorium69.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "Christiaan Ras",
            "email": "christiaan.ras@sensorium69.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sensorium\\LateralJoins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sensorium\\LateralJoins\\DatabaseServiceProvider"
            ]
        }
    }
}
